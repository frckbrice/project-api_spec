{
    "version": 2,
    "routes": [
        {
            "src": "/api-spec",
            "dest": "/senwisetool-api.yaml",
            "headers": {
                "Content-Type": "application/yaml",
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}